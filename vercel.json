{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "PHP_INI_SCAN_DIR": "api",
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "http://www.nexioarena.web.id/",
        "ASSET_URL": "http://www.nexioarena.web.id/",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "3zmy0.h.filess.io",
        "DB_PORT": "3307",
        "DB_DATABASE": "nexioarena_pressurein",
        "DB_USERNAME": "nexioarena_pressurein",
